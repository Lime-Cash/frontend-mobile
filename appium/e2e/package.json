    "appium": "^2.5.4",
    "appium-doctor": "^1.16.2",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "webdriverio": "^8.38.0"
  },
