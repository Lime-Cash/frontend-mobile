{
  "name": "frontend-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "expo lint",
    "cypress": "mkdir -p .cypress-cache && CYPRESS_CACHE_FOLDER=./.cypress-cache HOME=$PWD/.cypress-home TS_NODE_PROJECT=.cypress-tsconfig.json cypress open && npx expo start --web",
    "cypress:fix": "TS_NODE_PROJECT=.cypress-tsconfig.json cypress open",
    "cypress:local": "CYPRESS_CACHE_FOLDER=./node_modules/.cypress TS_NODE_PROJECT=.cypress-tsconfig.json cypress open",
    "cypress:all-local": "mkdir -p .cypress-cache && CYPRESS_CACHE_FOLDER=./.cypress-cache HOME=$PWD/.cypress-home TS_NODE_PROJECT=.cypress-tsconfig.json cypress open",
    "dev:test": "concurrently \"npm run web\" \"npm run cypress\"",
    "appium:server": "appium server --port 4723",
    "appium:driver:ios": "appium driver install xcuitest",
    "appium:driver:android": "appium driver install uiautomator2",
    "appium:doctor:ios": "appium-doctor --ios",
    "appium:doctor:android": "appium-doctor --android",
    "appium": "cd appium/e2e && npm run test",
    "appium:ios": "cd appium/e2e && npm run test:ios",
    "appium:android": "cd appium/e2e && npm run test:android",
    "appium:setup": "npm run appium:driver:ios && npm run appium:driver:android",
    "appium:doctor": "npm run appium:doctor:ios && npm run appium:doctor:android",
    "appium:dev": "concurrently \"npm start\" \"sleep 10 && cd appium/e2e && npm run test:ios\"",
    "appium:register": "cd appium/e2e && npm run test:register"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "axios": "^1.9.0",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-toast-message": "^2.3.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "19.0.0",
    "concurrently": "^9.1.2",
    "cypress": "^14.4.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.5",
    "prettier": "^3.5.3",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.3.3"
  },
  "private": true
}
